command /advancedores <text>:
	description: Advanced Ores Commands
	usage: &6/advancedores <help/reload/debug>
	aliases: /ao, /advancedore
	executable by: players, console
	trigger:
		if arg 1 is "help" or "h":
			player has permission "ao.staff.ao" or "ao.staff.*" or "ao.*":
				message "&4&m-------------------------------------------------"
				message "&cAdvanced Ores &b{@Version}"
				message "&6/ao help: &f(/ao h)&r Is this command"
				message "&6/ao reload: &f(/ao r)&r Reloads"
				message "&6/ao debug: &f(/ao d)&r Debug"
				message "&4&m-------------------------------------------------"
		else if arg 1 is "reload" or "r":
			player has permission "ao.staff.reload" or "ao.staff.*" or "ao.*":
				execute command "/skript reload Advanced Ores.sk"
				message "%{ao::messages::reload}%"
		else if arg 1 is "debug" or "d":
			player has permission "ao.staff.debug" or "ao.staff.*" or "ao.*":
				if {ao::command::debug} is not set:
					set {ao::command::debug} to true
					message "%{ao::messages::debug}%: %{ao::messages::on}%"
					log "Debug Enabled!" to "AdvancedOres/debug.log"
				else if {ao::command::debug} is true:
					clear {ao::command::debug}
					message "%{ao::messages::debug}%: %{ao::messages::off}%"
					log "Debug Disabled!" to "AdvancedOres/debug.log"
