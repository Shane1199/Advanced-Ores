on join:
	player has permission "ao.staff.update" or "ao.staff.*" or "ao.*":
		if {ao::config::check-for-updates} is true:
			wait 20 ticks
			if {@Version} contains "git":
				if text from "https://raw.githubusercontent.com/Shane1199/Advanced-Ores/master/Git-Version.txt" is not "{@Version}":
					message "%{ao::messages::prefix}% &cUh oh, Advanced Ores is outdated!"
					message "&bhttps://github.com/Shane1199/Advanced-Ores/releases"
			else:
				if text from "https://raw.githubusercontent.com/Shane1199/AdvancedOres/master/Version.txt" is not "{@Version}":
					message "%{ao::messages::prefix}% &cUh oh, Advanced Ores is outdated!"
					message "&bhttps://www.spigotmc.org/resources/17673/updates"
on load:
	send "[AO] Advanced Ores is Loading..." to console
	if folder "plugins/AdvancedOres/" doesn't exists:
		create folder "plugins/AdvancedOres"
		if {ao::command::debug} is true:
			log "Created Advanced Ores folder." to "AdvancedOres/debug.log"
	if file "plugins/AdvancedOres/messages.yml" doesn't exists:
		create file "plugins/AdvancedOres/messages.yml"
		wf "  ## Edit rewards here: plugins/Skript/scripts/AdvancedOres.sk" to "plugins/AdvancedOres/messages.yml"
		wf "  ## Formatting/Colors found here: http://minecraft.gamepedia.com/Formatting_codes" to "plugins/AdvancedOres/messages.yml"
		wf "  ## The player is said after BROADCAST & MESSAGE" to "plugins/AdvancedOres/messages.yml"
		wf "  ## SUPPORT <-> https://github.com/Shane1199/Advanced-Ores/issues" to "plugins/AdvancedOres/messages.yml"
		wf " " to "plugins/AdvancedOres/messages.yml"
		wf "prefix: '&cAO > '" to "plugins/AdvancedOres/messages.yml"
		wf "broadcastplayercolor: '&a'" to "plugins/AdvancedOres/messages.yml"
		wf "broadcast: '&9has randomly received&c'" to "plugins/AdvancedOres/messages.yml"
		wf "broadcastpart2: '&9while mining!'" to "plugins/AdvancedOres/messages.yml"
		wf "message: '&6You randomly found&c'" to "plugins/AdvancedOres/messages.yml"
		wf "messagepart2: '&6while mining!'" to "plugins/AdvancedOres/messages.yml"
		wf "reload: '&cAdvanced Ores &ahas been Reloaded.'" to "plugins/AdvancedOres/messages.yml"
		wf "debug: '&6Debug&7'" to "plugins/AdvancedOres/messages.yml"
		wf "on: '&eOn'" to "plugins/AdvancedOres/messages.yml"
		wf "off: '&eOff'" to "plugins/AdvancedOres/messages.yml"
		if {ao::command::debug} is true:
			log "Created messages.yml file." to "AdvancedOres/debug.log"
	else:
		set {ao::messages::prefix} to colored value "prefix" get of "plugins/AdvancedOres/messages.yml"
		if {ao::messages::prefix} is not set:
			wf "prefix: '&cAO > '" to "plugins/AdvancedOres/messages.yml"
			set {ao::messages::prefix} to colored value "prefix" get of "plugins/AdvancedOres/messages.yml"
			set {ao::c::aaom} to "prefix"
			execute console command "aaom"





			send "[AO] Error Messages.yml prefix was incorrectly configured! Reverting to default" to console
			if {ao::command::debug} is true:
				log "prefix incorrectly configured." to "AdvancedOres/debug.log"
		set {ao::messages::broadcastplayercolor} to colored value "broadcastplayercolor" get of "plugins/AdvancedOres/messages.yml"	
